# Terraform変数の設定例
# このファイルをコピーして terraform.tfvars として使用してください
# terraform.tfvars はGitに含まれません

aws_region = "ap-northeast-1"
app_name   = "leap-year-checker"
environment = "production"

# S3バケット名はグローバルに一意である必要があります
# 以下を自分の環境に合わせて変更してください
website_bucket_name = "your-unique-bucket-name-leap-year-app"
